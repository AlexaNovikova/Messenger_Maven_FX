<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="305.0" prefWidth="503.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controllers.AuthDialogController">
   <children>
      <Label layoutX="51.0" layoutY="60.0" prefHeight="37.0" prefWidth="81.0" text="Логин:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="51.0" layoutY="126.0" text="Пароль:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="loginField" layoutX="139.0" layoutY="53.0" prefHeight="39.0" prefWidth="316.0">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passField" layoutX="139.0" layoutY="114.0" onAction="#checkAuth" prefHeight="39.0" prefWidth="316.0">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <Button layoutX="191.0" layoutY="169.0" mnemonicParsing="false" onAction="#checkAuth" prefHeight="39.0" prefWidth="124.0" text="Вход">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="32.0" layoutY="239.0" mnemonicParsing="false" onAction="#registerOpen" prefHeight="51.0" prefWidth="188.0" text="Регистрация">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button layoutX="315.0" layoutY="239.0" mnemonicParsing="false" onAction="#openChangeNickView" prefHeight="51.0" prefWidth="157.0" text="Смена ника">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
